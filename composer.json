{
    "name": "irvyne/embedly-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 Embedly Bundle",
    "keywords": ["Symfony2", "Bundle", "Embedly"],
    "homepage": "https://github.com/Irvyne/IrvyneEmbedlyBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Thibaud Bardin",
            "email": "thibaud.bardin@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php":                           ">=5.3.3",
        "symfony/framework-bundle":      "~2.1",
        "symfony/monolog-bundle":        "~2.1",
        "symfony/twig-bundle":           "~2.1",
        "sensio/framework-extra-bundle": "~2.1",
        "embedly/embedly-php":           ">=0.5.1"
    },
    "autoload": {
        "psr-0": { "Irvyne\\EmbedlyBundle": "" }
    },
    "target-dir": "Irvyne/EmbedlyBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
